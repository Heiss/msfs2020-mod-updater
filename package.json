{
  "name": "msfs-2020-mod-updater",
  "version": "1.0.0",
  "description": "MSFS 2020 Mod Updater",
  "private": "true",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "vuetify",
    "desktop",
    "MSFS"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite-tauri dev",
