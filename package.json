    "build:web": "vite build",
    "build": "vite build && tauri build"
  },
  "dependencies": {
    "@mdi/font": "6.4.95",
    "@tauri-apps/api": "1.0.0-beta.8",
    "roboto-fontface": "0.10.0",
    "vue": "3.2.20",
    "vue-global-api": "0.4.1",
