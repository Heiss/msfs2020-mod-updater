    "vuetify": "3.0.0-alpha.11",
    "ynetwork": "3.0.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.10",
    "@vitejs/plugin-vue": "1.9.4",
    "@vuetify/vite-plugin": "1.0.0-alpha.5",
    "sass": "1.43.4",
    "sass-loader": "12.3.0",
    "vite": "2.6.13",
    "vite-plugin-tauri": "^0.1.6"
  },
  "repository": {
    "type": "github",
